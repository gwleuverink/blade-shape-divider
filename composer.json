{
    "name": "leuverink/blade-shape-divider",
    "description": "A blade component port of shapedivider.app",
    "keywords": [
        "blade-shape-divider",
        "Laravel",
        "Blade component",
        "Shape divider",
        "shapedivider.app"
    ],
    "homepage": "https://github.com/gwleuverink/blade-shape-divider",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Willem Leuverink",
            "email": "willem@leuver.ink",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "illuminate/support": "^9.0",
        "illuminate/view": "^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Leuverink\\BladeShapeDivider\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Leuverink\\BladeShapeDivider\\Tests\\": "tests"
        }
    },

    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Leuverink\\BladeShapeDivider\\BladeShapeDividerServiceProvider"
            ]
        }
    }
}
